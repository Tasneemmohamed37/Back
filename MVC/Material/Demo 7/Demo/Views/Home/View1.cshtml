<h1>Hello World FRom View </h1>
<!--HTML Css JS <= C#(Render)-->
<style>
    h1{
        color:red;
        background-color:yellow;
    }
</style>
<script>
console.log("Helloooooooooooooo");
</script>
<!--Razor Syntax (razor engin)-->
<!--start Block of Code-->
@{
    int x=10;
    int y=x+10;
}
<!--End of Block-->

<!--inline expression -->
<h2>@x</h2>
<h2>@(x+y)</h2>

<!--Condtion statment-->
@if (x < y)
{
    <h3> x =@x less than y =@y</h3>

}
else
{
    <h3> x greater than y</h3>
}
<!--Iteration-->
@for(int i = 0; i < 10; i++)
{
    <li>@i</li>    
}






